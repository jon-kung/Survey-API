{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "survey_id",
    "question"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "survey_id": {
      "$id": "#/properties/survey_id",
      "type": "integer",
      "title": "The Survey_id Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "question": {
      "$id": "#/properties/question",
      "type": "string",
      "title": "The Question Schema",
      "default": "",
      "examples": [
        "Do you like bananas?"
      ],
      "pattern": "^(.*)$"
    }
  }
}