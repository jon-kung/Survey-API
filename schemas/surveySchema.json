{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id",
    "category",
    "question",
    "choices"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "category": {
      "$id": "#/properties/category",
      "type": "string",
      "title": "The Category Schema",
      "default": "",
      "examples": [
        "test_category"
      ],
      "pattern": "^(.*)$"
    },
    "question": {
      "$id": "#/properties/question",
      "type": "string",
      "title": "The Question Schema",
      "default": "",
      "examples": [
        "This is a test question"
      ],
      "pattern": "^(.*)$"
    },
    "choices": {
      "$id": "#/properties/choices",
      "type": "array",
      "title": "The Choices Schema",
      "items": {
        "$id": "#/properties/choices/items",
        "type": "boolean",
        "title": "The Items Schema",
        "default": false,
        "examples": [
          true,
          false
        ]
      }
    }
  }
}